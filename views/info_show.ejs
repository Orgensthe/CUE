      <div class = "poster">

        <img src="images/2.png">

      </div>
      <div class = "summary">
        <table>
          <tr>
            <td>날짜 : </td>
            <td> <%= date %> </td>
          </tr>
          <tr>
              <td>장소 : </td>
              <td> <%= place %></td>
          </tr>
          <tr>
              <td>가격 : </td>
              <td> <%= price %></td>
          </tr>
          <tr>
              <td>시간 : </td>
              <td>  <%= starttime %> ~ <%= endtime%></td>
          </tr>
          <tr>
              <td>공지사항 : </td>
              <td><% introduce %>
              </td>
          </tr>
        </table>
      </div>
      <div class = "reserve">
        <input type="button" value="예약하기">
      </div>
    </div>


    <!--공연 내용에 대한 세부 정보 및 탭-->
    <div class = "tab">
