<!DOCTYPE html>
<html>
<head>
	<title>CUE!</title>

	<link rel="stylesheet" href="stylesheets/bootstrap.css">
  <link rel="stylesheet" href="stylesheets/bootstrap-grid.css">
  <link rel="stylesheet" href="stylesheets/menu3.css">
  <link rel="stylesheet" href="stylesheets/info_show.css">
  <link rel="stylesheet" href="stylesheets/search.css">
</head>

<body>
<header>
  <nav>
    <div class="logo_div">
    </div>

    <div class="search-icon">
      <img class="search" src="images/search.svg">
    </div>
    <div class="icons">
      <img class="mypageicon" src="images/mypageicon.svg">
    </div>

    <div class="search-drawer shadow-bar s-closed">
      <div class="search-tool">
        <div id="search-nav">
          <div class="search-close">X</div>
        </div>
        <div id="search-form">
          <form>
            <input class="in-search" type="text" name="s" value="" aria-required="false" autocomplete="on">
          </form>
        </div>
      </div>
    </div>

    <div class="overlay"></div>


    <div class="menu2">
      <div class='top'></div>
      <div class='middle'></div>
      <div class='bottom'></div>
    </div>
    <div class="menu-box">
      <div class="menu2-items">
